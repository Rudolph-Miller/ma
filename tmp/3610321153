            var elm = $(this);
            elm.find('input:radio').css({'left' : '-999em'});
            elm.find('.option').addClass('unselected')
            elm.find('.btnResearch').prop('disabled', true).addClass('disabled');
            elm.find('.option').click(function() {
                elm.find('.option').removeClass('selected').addClass('unselected');
                $(this).removeClass('unselected').addClass('selected');
            });
            elm.find('input:radio[name="answer"]').bind('change', function() {
                if(elm.find('input:radio[name="answer"]:checked').length > 0) {     
                   elm.find('.btnResearch').prop('disabled', false).removeClass('disabled');
                }
            });
        });
    }  
    iniForm(".voteForm");
});
</script>


</body>
</html><!-- web214.topics.kks.yahoo.co.jp uncompressed/chunked Thu May 29 11:59:13 JST 2014 -->
