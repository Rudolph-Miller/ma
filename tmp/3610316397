		$('#side .category .tab li:first').addClass('crnt');
		$('#side .category .tabcont:first').show();
		$('#side .category .tab li a').mouseover(function(){
			$('#side .category .tab li').removeClass('crnt');
			cnum = $(this).parent().attr('class');
			$(this).parent().addClass('crnt');
			$('#side .category .tabcont').hide();
			$('#side .category ul.'+cnum).fadeIn(300);
