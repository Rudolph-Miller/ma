		}
	}else{
		gaAction();
	}
}else{
	gaAction();
}
var spView = new Object();
spView.goToSPSite = function(){}
spView.checkUA = function(){
	if(ua.indexOf('iPhone') > 0 && ua.indexOf("FBID/tablet") == -1 || ua.indexOf('iPod') > 0 && ua.indexOf("FBID/tablet") == -1 || (ua.indexOf('Tablet') == -1 && ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0) || ua.indexOf('blackberry') > 0 || ua.indexOf('IEMobile') > 0){
		spView.goToSPSite = function(){
			document.cookie = "pcview=false; path=/;";
			location.href = location.protocol + "//" + location.hostname + "/sp" + location.pathname;
		}
