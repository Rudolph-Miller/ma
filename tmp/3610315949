      var msgs = {'loadMore': 'もっと読み込む...', 'loading': '読込中...', 'loaded': 'これ以上はありません&#12290;', 'addComment': 'コメントを追加', 'reply': '返信', 'delete': '削除'};
      var config = {'blogId': '5582672144555408512', 'postId': '8470741933523706183', 'feed': 'http://modern-cl.blogspot.com/feeds/8470741933523706183/comments/default', 'authorName': 'm2ym', 'authorUrl': 'http://www.blogger.com/profile/05102691172048785454', 'baseUri': 'http://www.blogger.com', 'maxThreadDepth': 2};

