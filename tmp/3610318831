<script type="text/javascript">
function sendParentResizeMessage(context) {
    if (context && context.parentElementId) {
        if (parent && parent.postMessage) {
            // Measure content width and height
            var contentWidth = 305;
            var contentHeight = 840;
            try {
                var adsDivEl = document.getElementById('widgetElement');
                contentWidth = adsDivEl.clientWidth ? adsDivEl.clientWidth : adsDivEl.offsetWidth;
                contentHeight = adsDivEl.clientHeight ? adsDivEl.clientHeight : adsDivEl.offsetHeight;
            } catch (e) {
                contentWidth = 305;
                contentHeight = 840;
            }
            // Add the search result count
            var parentElementId = context.parentElementId;
            var message = 'method=updateAdSpace'
            + '&contentWidth=' + contentWidth
            + '&contentHeight=' + contentHeight
            + '&elementId=' + encodeURIComponent(parentElementId);
            // It's safe to use '*' as targetOrigin here, because the message content is not secret.
            window.parent.postMessage(message, '*');
        }
    }
}

var myHashArguments = cX.library.parseHashArgs();
var adDataIb;
var adDataMg;
var recsData;

function renderWidget() {   
    cX.library.renderTemplate('relationTemp', 'cxRecentries');
    var context = myHashArguments;
    sendParentResizeMessage(myHashArguments );
}

//
// IBAds renderFunction
//
function onGotIbAdData(data, context) {
    adDataIb = data;
    if (typeof recsData !== 'undefined' && typeof adDataMg !== 'undefined') {
        renderWidget();
    }
}

//
// MGAds renderFunction
//
function onGotMgAdData(data, context) {
    adDataMg = data;
    if (typeof recsData !== 'undefined' && typeof adDataIb !== 'undefined') {
        renderWidget();
    }
}

//
// Recs renderFunction
//
function onGotRecsData(data) {
    recsData = data;
    if (typeof adDataMg !== 'undefined' && typeof adDataIb !== 'undefined') {
        renderWidget();
    }
}

// mgpr
function appendRecentEntries(){
	$.ajax({
		type: "GET",
		dataType: "json",
		url: "/common/json/pr_article.json"+"?"+(new Date().getTime()),
		success: function(data){
			var $cxRec = $("#cxRecentries");
			var $cxRecList = $cxRec.find("li");
			var cxRecLen = $cxRec.find("li").length;
			var prLen = data.article.length;
			var prAr = [];
			var articleNum = prLen < 3 ? prLen : 3;
			for(var i=0; i<prLen; i++){
				prAr[i] = i;
			}
			prAr = prAr.sort(function() {
				return Math.random() - Math.random();
			});
			for(var i=0; i<articleNum; i++){
				var r = Math.floor( Math.random() * cxRecLen );
				$cxRecList.eq(r).before(data.article[prAr[i]]);
			}
		},
		error : function(data) {}
	});
}

//IBAds setup
cX.library.insertAdSpace({
    adSpaceId: '000000016017b16c', // TODO
    backend:'production', 
    forwardHashArgs: true,
    renderFunction: onGotIbAdData
});

// MGAds setup
cX.library.insertAdSpace({
    adSpaceId: '000000016002e85c', // TODO
    backend:'production', 
    forwardHashArgs: true,
    renderFunction: onGotMgAdData
});

cX.library.insertWidget({
    //TODO
    widgetId: '778ad35366f53fbd86e09b39d8e00cec1b6ae7b4',
    renderFunction: onGotRecsData
});

