			function searchbox_Keypress(event) {
				if(event != undefined && (event.keyCode== 13 || event.which == 13 || event.charCode == 13)) {
					doSearchIntegration('searchbox', 'http://search.jp.reuters.com/query/', ''); 
					return false;
				}
			}
			function searchbox_Onfocus() {
				clearSearch(frmDefault_searchbox, '株価, ニュース, ビデオ');
			}
			function searchbox_Onblur() {
				fillSearch(frmDefault_searchbox, '株価, ニュース, ビデオ');
			}
			document.getElementById('frmDefault').onsubmit = frmDefault_Submit;
			document.getElementById('frmDefault_goBtn').onclick = goBtn_Click;
