}

var uaSniff = navigator.userAgent;
if ((uaSniff.match(/iPhone/i)) || (uaSniff.match(/iPod/i)) || (uaSniff.match(/android/i))) {
    sa_makeAd();
    setTimeout('sa_startEngine()', 1500);
}
function sa_startEngine() {
    sa_updateAdLayout();
    var sa_int = self.setInterval(sa_updateAdLayout, 500);
}
