function sa_calcAdSize() {
    sa_adW = sa_defAdW;  
    sa_adH = sa_defAdH; 
}

function sa_updateAdLayout() {
    sa_calcAdSize();
    var sa_tgtDiv = document.getElementById('spAdCode');
    sa_tgtDiv.style.backgroundColor = "transparent";
    sa_tgtDiv.style.display = "block";
    sa_tgtDiv.style.position = "absolute";
    sa_tgtDiv.style.top = "0";
    sa_tgtDiv.style.zIndex = "999";
