<script type="text/javascript">
$(function() {
    (function($, exports) {

        var $el = $('#comframe'),
            keys = $el.attr('data-keys'),
            comment_num = $el.attr('data-comment_num'),
            reply_num = $el.attr('data-reply_num'),
            full_page_url = $el.attr('data-full_page_url') || '',
            url = 'http://pcache.news.yahoo.co.jp/v1/comment/short/';

        url += '?keys=' + keys + '&comment_num=' + comment_num + '&reply_num=' + reply_num + '&full_page_url=' + full_page_url;

        if (typeof YAHOO ==='undefined' || !YAHOO) {
            YAHOO = {};
        }

        if (typeof YAHOO.JP ==='undefined' || !YAHOO.JP) {
            YAHOO.JP = {};
        }

        if (typeof YAHOO.JP.News ==='undefined' || !YAHOO.JP.News) {
            YAHOO.JP.News = {};
        }
        
        if (typeof YAHOO.JP.News.comment ==='undefined' || !YAHOO.JP.News.comment) {
            YAHOO.JP.News.comment = {};
        }

        var _callback = function(param) {

            iframe = $('#commentIframe'),
            _scrollpos = 0,
            _type = param.split('-')[0],
            _param = param.split('-')[1],
            _init = function(param) {

                $('#loadingComment').remove();

                iframe.height(param);
            };

            switch (_type) {
            case 'postcomment':

                _scrollpos = iframe.offset().top + ~~_param * 0.9;
                target = ($('body').scrollTop() >= $('html').scrollTop()) ? $('body') : $('html');
                target.animate({scrollTop: _scrollpos}, 'slow', 'swing');

                break;
            case 'adjustheight':

                iframe.height(_param);

                break;
            case 'init':

                _init(_param);

                break;
            default :

                _init(_param);

                break;
            }
        };

        exports.YAHOO.JP.News.comment = (function() {
            return {
                callback: function(param) {
                    var _param = param.substring(1);
                    _callback(_param);
                },
                parse: function() {
                    $('<iframe>').attr({'src':url, 'class':'commentIframe', 'id':'commentIframe', 'scrolling':'no', 'frameborder':'0', 'height':'0'}).appendTo('#comframe');
                }
            };
        })();

    })(jQuery, window);
});

if (typeof(YJNEWS) !== 'undefined' && typeof(YJNEWS.TAB) !== 'undefined') {
    YJNEWS.TAB.setTabProperty({
        'tabId'         : 'commentTab',
        'contentsId'    : 'commentTabContents',
        'prefunc'       : getComment,
        'owner'         : window,
        'rdLabelCngTab' : '/media/news/tab/click/comment/pc/textnews/',
        'rdLabelImp'    : '/media/news/tab/imp/comment/pc/textnews/'
    });
} else {
    getStatus();
    getComment();
}
</script>

