            $('#credInitFlag').attr('init_flag', 'true');
        };

        YAHOO.JP.credstore.config = {
            cs_verifier: '-500',
            callback: '',
            redirectUrl: 'http://buzz.news.yahoo.co.jp/article/b0095feb0afd7fb8819db9960e3218a82d56c77d/',
            src: 'yn',
            confirmation: 'none',
            initCallback: initCallback
        };
    }

    window.YAHOO = YAHOO;
    if (window.addEventListener) {
        window.addEventListener('load', f, false);
    } else {
        window.attachEvent('onload', f, false);
    }
    s.parentNode.insertBefore(js, s);

    if (window.addEventListener) {
        window.addEventListener('message', function(event) {
            if (event.origin === 'https://accountlink.yahoo.co.jp') {
                var _data = JSON.parse(event.data);
                if (_data['status'] === 'cs:save_preferences') {
                    location.href = 'http://buzz.news.yahoo.co.jp/article/b0095feb0afd7fb8819db9960e3218a82d56c77d/';
                }
            }
        }, false);
    } else {
        window.attachEvent('onmessage', function(event) {
            if (event.origin === 'https://accountlink.yahoo.co.jp') {
                var params = data.slice(1, - 2).replace(/"/g, '').split(','),
                len = params.length,
                _data = {},
                delim,
                i;

                for (i = 0; i < len; i++) {
                    delim = params[i].indexOf(':');
                    _data[params[i].slice(0, delim)] = params[i].slice(delim + 1);
                }
                if (_data['status'] === 'cs:save_preferences') {
                    location.href = 'http://buzz.news.yahoo.co.jp/article/b0095feb0afd7fb8819db9960e3218a82d56c77d/';
                }
            }
        });
    }


