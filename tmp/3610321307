        s = document.getElementsByTagName('script')[0],
        f = function(){YAHOO.JP.credstore.ready = true;};

    var userAgent = window.navigator.userAgent.toLowerCase();

    js.src = 'https://s.yimg.jp/images/yjdn/CredStore/plugin/js/accountlink-link.js';
    js.type = 'text/javascript';
    js.id = 'accountlink-link-js';

    // IE
    if (userAgent.indexOf('msie') != -1) {

        YAHOO.JP.credstore.config = {
            cs_verifier: '-500',
            callback: '',
            redirectUrl: 'http://buzz.news.yahoo.co.jp/article/6d85b57919cb6da6d42e0402d4cbdbf68dc4d0cf/',
            src: 'yn',
            confirmation: 'none'
        };

